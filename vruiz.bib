@InProceedings{ORTIZ10b,
  author =       {García-Ortiz, J.P. and
                  González-Ruiz, V. and
                  García, I. and
		  M\"{u}ller, D. and
		  Dimitoglou, G.},
  url =          {http://dx.doi.org/10.1109/ICPR.2010.778},
  title =        {Interactive Browsing of Remote JPEG 2000 Image Sequences},
  booktitle =    {International Conference on Pattern Recognition (ICPR)},
  pages = 	 {3179-3182},
  year = 	 {2010},
  address = 	 {Istambul, Turkey},
  month = 	 {August},
  publisher =    {IEEE press},
  quality =      {\href{http://core.edu.au/cms/images/downloads/conference/08sorttitleERA2010_conference_list.pdf}{CORE B}},
  quality_long = "\newline\textbf{Indicios de calidad:} El congreso
                  IEEE ICPR (International Conference on Pattern
                  Recognition) está clasificado en el CORE (COmputer
                  Research AND Education) Conference Ranking de 2008,
                  como \textbf{CORE B}
                  (\href{http://core.edu.au/cms/images/downloads/conference/08sorttitleERA2010_conference_list.pdf}
                  {\texttt{http://core.edu.au}})",
  sexenio = {En este trabajo se presenta y evalúa un modelo de
                  precarga de datos que maximiza la calidad de la
                  transmisión interactiva de secuencias de imágenes
                  \href{http://www.jpeg.org/jpeg2000/}{JPEG 2000}, en
                  la que un usuario alterna entre dos modos de
                  visualización: el modo imagen (donde una imagen es
                  transmitida progresivamente mientras se inspecciona)
                  y el modo vídeo (donde las imágenes de la secuencia
                  son presentada como si de un vídeo se tratase a un
                  determinado frame-rate y calidad, proporcional al
                  ancho de banda disponible). Dicho modelo se basa en
                  que la mayoría de las imágenes naturales presentan
                  una curva R/D (Rate/Distortion) mucho más
                  pronunciada al comienzo de la transmisión que al
                  final, es decir, que la mejora de la calidad en una
                  reconstrucción progresiva es más acuciada al
                  comienzo de la transmisión. Por este motivo, cuando
                  el usuario utiliza el modo imagen durante cierto
                  tiempo, la calidad de la visualización de la
                  secuencia completa puede optimizarse si en lugar de
                  seguir transmitiendo datos de la imagen que se está
                  visualizando se transmiten datos de las imágenes
                  vecinas. Los resultados demuestran que en
                  condiciones normales, el uso del modelo aumenta la
                  calidad de la experiencia del usuario. Este modelo
                  tiene aplicación en cualquier sistema interactivo de
                  streaming de vídeo escalable, como por ejemplo,
                  \href{http://jhelioviewer.org}{Jhelioviewer}.}
}

