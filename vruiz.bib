@ARTICLE (ORTIZ04a,
	AUTHOR = "J.P. Ortiz and V.G. Ruiz and I. Garc{\'\i}a",
	TITLE = "Improving {W}eb proxy caching on browsing
                  {JPEG2000} remote images with {JPIP}",
        url = {http://www.hpca.ual.es/~vruiz/papers/ORTIZ04a.pdf},
	JOURNAL = "ACTA Press",
	VOLUME = "1",
	NUMBER = "1",
	PUBLISHER = "World Scientific and Engineering Academy and Society",
	PAGES = "167-172",
 	ISSN = "1790-0832",
	MONTH = "July",
        impact =  {$0.034$ (SCImago Journal Rankings 2006)},
        quality_long = "\newline\textbf{Indicios de calidad:} Todos los
                  artículos publicados en los libros y revistas
                  periódicas editadas por WSEAS se revisan por tres
                  expertos de renombre internacional antes de ser
                  aceptados. Esta revista además se encuentra indexada
                  por INSPEC, SCImago y Scopus. Según los datos del
                  \textbf{SCImago Journal Rankings}
                  (\href{http://www.scimagojr.com/journalrank.php}
                  {\texttt{http://www.scimagojr.com}}) a fecha de 2006
                  (el primer año que hay datos disponibles), la
                  revista WSEAS Transactions on Information Science
                  and Applications posee un indicador SJR de
                  $\textbf{0.034}$, apareciendo en el puesto 47 de 58
                  revistas en la categoría COMPUTATIONAL MATHEMATICS y
                  en el puesto 77 de 108 revistas en la categoría
                  COMPUTER SCIENCE (MISCELLANEOUS). Por otra parte,
                  indicar que la línea de trabajo que básicamente se
                  incia en esta publicación es la que finalmente, en
                  2010 da lugar a un contrato
                  (\href{http://www.20minutos.es/noticia/878123/0/}{\texttt{http://www.20minutos.es/noticia/878123/0}},
                  \href{http://www.ideal.es/almeria/20101118/local/almeria/nasa-ficha-equipo-cientificos-201011181456.html}{\texttt{http://www.ideal.es/almeria/20101118/local/almeria/\-nasa-ficha-equipo-cientificos-201011181456.html}})
                  entre la empresa IPTV Solutions
                  (\href{http://www.iptv-solutions.es}{\texttt{http://www.iptv-solutions.es}})
                  y la ESA (European Space Agency) para desarrollar un
                  servidor de imágenes JPEG 2000 que va a ser usado en
                  el proyecto JHelioviewer
                  (\href{http://www.jhelioviewer.org/}{\texttt{http://www.jhelioviewer.org}})",
  number_of_cites = {1 (\href{http://scholar.google.es/scholar?hl=es&q=%22Improving+Web+proxy+caching+on+browsing+JPEG2000+remote+images+with+JPIP%22&btnG=Buscar&lr=&as_ylo=&as_vis=0}{\texttt{http://scholar.google.es}})},
	YEAR = "2004",
  es_abstract = "\newline\textbf{Resumen:} Los servidores JPIP
                  mantienen un modelo de la caché de los clientes con
                  el fin de evitar el envío de información redundante
                  en el caso en que un cliente realice dos peticiones
                  de dos WOIs (Window Of Interest) solapadas. Esto
                  genera el inconveniente de que las respuestas JPIP
                  no pueden cacheadas deacuadamente por un proxy Web
                  porque dos peticiones iguales pueden ser servidas de
                  forma diferente. En este trabajo se presenta una
                  modificación del protocolo JPIP que afecta a la
                  forma en que los clientes demandan las WOIs, de
                  manera que se conserva la correspondencia unívoca
                  petición-respuesta, permiendo así el uso de proxies
                  Web para cachear respuestas JPIP"
)
