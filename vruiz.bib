@Article{GARCIA-VILCHEZ11,
  author = 	{García-Vílchez, Fernando and
                 Muñoz-Mar\'{i}, Jordi and
		 Zortea, Maciel and
		 Blanes, Ian and
                 González-Ruiz, Vicente and
		 Camps-Vall, Gustavo and
                 Plaza, Antonio and
		 Serra-Sagrist\`a, Joan},
  title =        {On the Impact of Lossy Compression on Hyperspectral Image Classification and Unmixing},
  journal = 	 {IEEE Geoscience and Remote Sensing Letters},
  year = 	 {2011},
  volume = 	 {8},
  number = 	 {2},
  pages = 	 {253-257},
  month = 	 {March},
  url =          {http://dx.doi.org/10.1109/LGRS.2010.2062484},
  impact =       {$1.560$ (ISI JCR 2011)},
  rankings =     {36th of 76 (Q2) journals in GEOCHEMISTRY AND GEOPHYSICS,
                  ENGINEERING, 71th of 245 (Q2) journals in ELECTRICAL AND
                  ELECTRONIC, 7th of 24 (Q2) journals in REMOTE SENSING
                  (ISI JCR 2011)},
  history =      {Submited October 19, 2009; revised March 7, 2010 and
                  June 6, 2010; accepted July 15, 2010},
  ISSN =         {1545-598X},
  quality_long = "\newline\textbf{Indicios de calidad:} Según el
                  \textbf{Journal Citation Reports} del ISI Web of
                  Knowledge de 2009, la revista IEEE Geoscience and
                  Remote Sensing Letters posee un índice de impacto de
                  $\textbf{1.379}$, encontrándose en el puesto 36 de
                  75 revistas en la categoría GEOCHEMISTRY AND
                  GEOPHYSICS, en el puesto 82 de 246 revistas en la
                  categoría ENGINEERING, ELECTRICAL AND ELECTRONIC y en
                  el puesto 7 de 21 revistas en la categoría REMOTE
                  SENSING",
  sexenio =      {Este trabajo supone un avance en el estudio del efecto de la compresión irreversible de imágenes hiperespectrales, a la hora de estimar de forma automática el número de endmembers (sustancias espectralmente puras), clasificarlos y calcular sus abundancias. Las imágenes hiperespectrales ocupan grandes cantidades de memoria y por ese motivo son susceptibles de ser sometidas a altas tasas de compresión. Sin embargo, la aplicación de algoritmos de clasificación y de desmezclado en imágenes hiperespectrales comprimidas con pérdidas podría provocar resultados erroneos dependiendo del contenido de las imágenes y de las tasas de compresión. En este artículo se analiza hasta qué punto las imágenes pueden ser comprimidas usando técnicas estándar como son JPEG \href{http://www.jpeg.org/jpeg2000/}{JPEG (Joint Photographic Experts Group) 2000}, CCSDS-IDC (Consultative Committee for Space Data Systems - Image Data Compression) y otros algoritmos específicamente diseñados para este tipo de imágenes como es el que se implementa en el \href{http://gici.uab.es/TER/}{software TER}. Los resultados muestran que la compresión con pérdidas puede afectar negativamente a la estimación del número de endmembers y extracción de los mismos calculados por algoritmos como NWHFC (Noise Whithed - Harsanyi, Farrand and Chang) y HySime (HYperspectral Signal subspace Identification by Minimum Error). Por el contrario, la degradación de las clasificaciones realizadas por un clasificador SVM (Support Vector Machine) basado en pixels es mínima, incluso a altas de compresión. Finalmente los experimentos realizados constatan la superioridad de JPEG 2000 (y extensiones de éste) frente a otros compresores de imágenes para codificar irreversiblemente imágenes hiperespectrales.},
  cites = {5 (Web of Science), 9 (Scopus), 11 (Scholar)}
}

