@Article{MARTIN10,
  author = 	 {Martín, Gabriel and
                  González-Ruiz, Vicente and
                  Plaza, Antonio and
                  Ortiz, Juan P. and
                  García, Inmaculada},
  title =        {Impact of JPEG2000 compression on endmember extraction and unmixing of remotely sensed hyperspectral data},
  journal = 	 {SPIE Journal of Applied Remote Sensing},
  year = 	 {2010},
  volume = 	 {4},
  number = 	 {041796},
  pages = 	 {1-24},
  month = 	 {July},
  url =          {http://dx.doi.org/10.1117/1.3474975},
  issn =         {1931-3195},
  impact =       {$1.000$ (ISI JCR 2010)},
  rankings =     {11th out of 22 (Q2) journals in REMOTE SENSING; 7th out
                  of 19 journals in IMAGING SCIENCE AND PHOTOGRAPHIC
                  TECHNOLOGY; 144rd out of 193 journals in
                  ENVIRONMENTAL SCIENCES (ISI JCR 2010)},
  history =      {Submited 28 November 2009; revised 1 March 2010;
                  accepted 19 April 2010; published 14 July 2010},
  sexenio =      {Este trabajo investiga la fiabilidad de la extracción
                  de endmembers (sustancias espectralmente puras) en
                  imágenes hiperespectrales comprimidas de forma
                  irreversible usando JPEG 2000, en función del tipo
                  de información (espacial y/o espectral)
                  utilizada. Los algoritmos de extracción y estimación
                  de abudancias de los endmembers son OSP (Orthogonal
                  Subspace Projection), AMEE (Automatic Morphological
                  Endmember Extraction) y SSEE (Spatial Spectral
                  Endmember Extraction). OSP utiliza sólo información
                  espectral, mientras que AMEE y SSEE utilizan tanto
                  la información espectral como la espacial. Además,
                  la forma en que AMEE y SSEE utilizan dichas fuentes
                  de información difiere. En el caso de SSEE primero
                  se utiliza la informacion espacial y luego la
                  espacial, mientras que en AMEE ambas fuentes de
                  información se tienen en cuenta durante todo el
                  proceso de estimación. Los resultados demuestran que
                  los tres métodos son bastante fiables al ser
                  aplicados sobre imágenes degradadas por la
                  compresión, lo que indica que JPEG 2000 preserva muy
                  bien la información espectral. Sin embargo, de los
                  tres métodos, el más eficiente es AMEE, lo que
                  confirma que el uso simultáneo de ambas fuentes de
                  información es además la opción más conveniente.},
  cites = {1 (Web of Science), 2 (Scopus), 3 (Scholar)},
}

