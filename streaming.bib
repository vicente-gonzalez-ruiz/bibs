@INPROCEEDINGS{Mehrotra, 
  author={Mehrotra, S. and Weidong Zhao}, 
  booktitle={IEEE International Workshop on Multimedia Signal Processing (MMSP)}, 
  title={Rate-distortion optimized client side rate control for adaptive media streaming}, 
  year={2009}, 
  pages={1-6}, 
  abstract={Media streaming over unreliable networks such as the
                  Internet is growing in popularity, but presents
                  unique challenges when trying to get the user
                  experience to be on par with classical mediums such
                  as cable television. These networks have variable
                  network conditions which not only vary between a set
                  of points in the network, but also change over
                  time. In this paper, we present a rate-distortion
                  (R-D) optimized algorithm for adapting the bitrate
                  of streaming media from a chunked encoding, where
                  each chunk is available at multiple bitrates (or
                  quality levels) or is scalable coded. The
                  optimization takes into account the desired startup
                  latency, the desired client buffer size, the current
                  client buffer size, and the estimated network
                  bandwidth. The problem is formulated as a distortion
                  minimization problem subject to multiple rate
                  constraints. For video content, the solution gives a
                  gain of up to 3-4 dB in PSNR in difficult portions
                  of the video when compared to commonly used
                  adaptation techniques and can achieve an arbitrarily
                  small desired startup latency.}, 
  keywords={Internet;media streaming;rate distortion theory;video on
                  demand;Internet;adaptive media streaming;chunked
                  encoding;distortion minimization problem;network
                  bandwidth;rate-distortion optimized client side rate
                  control;Adaptive control;Bandwidth;Bit rate;Cable
                  TV;Delay;Encoding;IP networks;Programmable
                  control;Rate-distortion;Streaming media}, 
  doi={10.1109/MMSP.2009.5293246}, 
  month={October}
}

